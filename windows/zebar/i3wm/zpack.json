{
  "$schema": "https://github.com/glzr-io/zebar/raw/v3.1.1/resources/zpack-schema.json",
  "name": "i3wm",
  "version": "0.0.0",
  "description": "",
  "tags": [],
  "previewImages": [],
  "repositoryUrl": "",
  "widgets": [
    {
      "name": "i3wm",
      "htmlPath": "./i3wm.html",
      "zOrder": "top_most",
      "shownInTaskbar": true,
      "focused": false,
      "resizable": false,
      "transparent": false,
      "includeFiles": [
        "**/**"
      ],
      "caching": {
        "defaultDuration": 604800,
        "rules": []
      },
      "privileges": {
        "shellCommands": []
      },
      "presets": [
        {
          "name": "i3wm",
          "anchor": "bottom_center",
          "offsetX": "0px",
          "offsetY": "0px",
          "width": "100%",
          "height": "20px",
          "monitorSelection": {
            "type": "primary"
          },
          "dockToEdge": {
            "enabled": true,
            "edge": "bottom",
            "windowMargin": "40px"
          }
        }
      ]
    }
  ]
}
